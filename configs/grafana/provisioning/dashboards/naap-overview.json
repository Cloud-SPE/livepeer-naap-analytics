          "pluginVersion": "4.12.0",
          "queryType": "table",
          "rawSql": "WITH request_latencies AS (\n    SELECT\n        request_id,\n        orchestrator_address,\n        min(event_timestamp) as start_time,\n        -- Calculate ms diff between first and last trace for this ID\n        dateDiff('millisecond', min(data_timestamp), max(data_timestamp)) AS latency_ms\n    FROM livepeer_analytics.stream_trace_events\n    WHERE $__timeFilter(event_timestamp)\n    GROUP BY request_id, orchestrator_address\n)\nSELECT\n    toStartOfInterval(start_time, INTERVAL 5 MINUTE) AS t,\n    orchestrator_address,\n    avg(latency_ms) AS avg_e2e_latency_ms\nFROM request_latencies\nGROUP BY t, orchestrator_address\nORDER BY t ASC",
          "refId": "A"
        }
      ],
      "title": "End-to-End (e2e) Stream Latency",
      "type": "timeseries"
