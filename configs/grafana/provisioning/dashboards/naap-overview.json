          "queryType": "sql",
          "rawSql": "SELECT\n  orchestrator_address,\n  count(DISTINCT stream_id) as active_streams,\n  avg(output_fps) as avg_fps,\n  quantile(0.95)(output_fps) as p95_fps,\n  sum(restart_count) as total_restarts\nFROM livepeer_analytics.ai_stream_status\nWHERE event_timestamp >= now() - INTERVAL 1 HOUR\nGROUP BY orchestrator_address\nORDER BY active_streams DESC\nLIMIT 10",
          "refId": "A"
        }
